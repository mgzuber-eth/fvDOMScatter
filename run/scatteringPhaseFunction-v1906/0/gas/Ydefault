FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/gas";
    object      Ydefault;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0]; 


internalField   uniform 0;


boundaryField
{
    bottomPlate
    {
        type            zeroGradient;
    }
    
	topPlate
    {
        type            zeroGradient;
    }
	
	plateWall
    {
        type            zeroGradient;
    }
}


